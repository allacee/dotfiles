#!/bin/sh
xset -q | tr '\n' ' ' | awk '{switch ($12){case "00000000": print "🇺🇸"; break; case "00000001": print "🇺🇸🅰️"; break; case "00000002": print "🇺🇸🔢"; break; case "00000003": print "🇺🇸🅰️🔢"; break; case "00001000": print "🇷🇺"; break; case "00001001": print "🇷🇺🅰️"; break; case "00001002": print "🇷🇺🔢"; break; case "00001003": print "🇷🇺🅰️🔢"; break;  default: print "UNKWN LED"; break;}}'
